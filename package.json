    "codecov": "^2.3.0",
    "eslint": "^4.7.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-flowtype-errors": "^3.3.1",
